{
    "manifest_version": 2,
    "author": "Sam @yungsamd17",
    "name": "Volume Control",
    "version": "1.1.1",
    "description": "Adjust volume up to 200%",
    "homepage_url": "https://github.com/yungsamd17/Volume-Control",

    "icons":
    {
            "16": "assets/icons/blue/16.png",
            "32": "assets/icons/blue/32.png",
            "64": "assets/icons/blue/64.png",
            "128": "assets/icons/blue/128.png"
    },
    "permissions": [
        "<all_urls>",
        "storage",
        "tabCapture",
        "nativeMessaging",
        "tabs"
    ],
    "background":
    {
        "persistent": true,
        "scripts": [
            "background.js"
        ]
    },
    "browser_action":
    {
        "default_title": "Volume Control",
        "default_popup": "popup.html"
    },
    "options_ui":
    {
        "page": "options/options.html",
        "browser_style": true,
        "chrome_style": true
    },
    "content_scripts": [
    {
      "matches": ["*://*/*"],
      "all_frames": true,
      "match_origin_as_fallback": true,
      "js": ["content.js"]
    }
  ]
}